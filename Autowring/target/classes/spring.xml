<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    
    <bean id="student" class="com.jsp.spring.Autowring.Student">
    <constructor-arg name="studentName" value="Loki" type="java.lang.String"></constructor-arg>
    <constructor-arg name="sId" value="101" type="int"></constructor-arg>
    </bean>
    
    <bean id="sName" class="com.jsp.spring.Autowring.Student">
    <constructor-arg name="sId" value="1" type="int"></constructor-arg>
    </bean>
    
    </beans>